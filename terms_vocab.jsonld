{
    "@context": ["https://mobi-dlts.github.io/custom_terms_vocab/terms_vocab.jsonld#"],
    "euConformityDeclarationDocument": {
      "@id": "http://schema.org/url", 
      "@type": "@id",
      "description": "URI of the EU declaration of conformity signed by responsible economic operators to declare compliance with the regulatory requirements in the context of the market conformity assessment procedure and assume full responsibility."
    },
    "separateCollectionSymbol": {
      "@id": "http://schema.org/url", 
      "@type": "@id",
      "description": "URI to the separate collection' or 'WEEE label' indicating that a product should not be discarded as unsorted waste but must be sent to separate collection facilities for recovery and recycling."
    },
    "cadmiumLeadSymbols": {
      "@id": "http://schema.org/url", 
      "@type": "@id",
      "description": "URI to the image of Cadmium and lead symbols indicating the metal is contained in the battery above a defined threshold."
    },
    "certifiedUsableBatteryEnergy": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "The energy supplied by the battery from the beginning of the test procedure used for certification until the applicable break-off criterion of the test procedure used for certification is reached. Unit of Measure is Kilo Watt Hour (kWh)."
    },
    "initialSelfDischargingRate": {
      "@id": "https://schema.org/Number", 
      "@type": "@id",
      "description": "Initial self-discharge in % of capacity per unit of time in defined conditions (temperature range etc) as pre-use metric."
    },
     "ratedCapacity": {
        "@id": "https://schema.org/Integer", 
        "@type": "@id",
        "description": "The total number of ampere-hours (Ah) that can be withdrawn from a fully charged battery under specific conditions. Unit of Measure is Ampere Hour (Ah)."    
    },
    "nominalVoltage": {
        "@id": "https://schema.org/Integer", 
        "@type": "@id",
        "description": "The voltage value assigned to an electric circuit or system to designate its voltage class. Unit of Measure is Volt."    
    },
    "minimumVoltage": {
        "@id": "https://schema.org/Integer", 
        "@type": "@id",
        "description": "Minimum voltage the battery is rated for. Unit of Measure is Volt."
    },
    "maximumVoltage": {
        "@id": "https://schema.org/Integer", 
        "@type": "@id",
        "description": "Maximum voltage the battery is rated for. Unit of Measure is Volt."
    },
    "originalPowerCapability": {
        "@id": "https://schema.org/Integer", 
        "@type": "@id",
        "description": "Original power capability (in Watts) and limits, with temperature range when relevant. Unit of Measure is Watt."
    },
    "maxPermittedBatteryPower": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Maximum permitted power the battery is rated for, includes the data relevant for 'power limits'. Unit of Measure is Watt."
    },
    "ratioNominalAllowedPower": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Ratio between nominal allowed battery power (W) and battery energy (Wh). Unit of Measure is Watts per Hour."
    },
    "initialRoundTripEnergyEfficiency": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "The ratio of the net energy delivered by a battery during a discharge test to the total energy required to restore the initial State of Charge by a standard charge."
    },
    "roundTripEnergyEfficiency50pCycleLife": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Round trip energy efficiency at 50% of cycle-life; measured at 50% of cycle life as determined in a pre-use standardized measurement."
    },
    "initialInternalResistanceCellLevel": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Initial (pre-use) internal resistance on battery cell level. Unit of Measure is Milliohm."
    },
    "maxInitialInternalResistancePackLevel": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Initial (pre-use) internal resistance on battery pack level. Unit of Measure is Milliohm."
    },
    "maxInitialInternalResistanceModuleLevel": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Initial (pre-use) internal resistance on battery module level. Unit of Measure is Milliohm."
    },
    "expectedLifetime": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Expected battery lifetime expressed in cycles, and reference test used. Unit of Measure is Number of Cycles."
    },
    "cRateRelevantCycleLifeTest": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Measurement parameter: Applied charge and discharge rate (C-rate) of relevant cycle-life test."
    },
    "soceExhaustionThreshold": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Minimum percentage of SOCE, above which the battery is still considered operational as EV battery in its current life."
    },
    "batteryWarrantyPeriod": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Warranty period of the battery."
    },
    "idleStateTemperatureRangeUB": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Upper boundary of the surrounding temperature range, which the battery can safely withstand. Unit of Measure is Celsius."
    },
    "totalEnergy": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Total amount of energy when charged to 100%. Unit of Measure is KiloWatt Hour."
    },
    "maxChargeRate": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "The highest sustainable amount of power the battery is capable of capturing. Unit of Measure is Kilowatt."
    },
    "maxDischargeRate": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "The maximum power at which thebattery can be discharged continuously. Unit of Measure is Kilowatt."
    },
    "operationalTempUB": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Upper boundary of the surrounding temperature range, which the battery can safely operate. Unit of Measure is Celsius."
    },
    "operationalTempLB": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Lower boundary of the surrounding temperature range, which the battery can safely operate. Unit of Measure is Celsius."
    },
    "storageHumidity": {
      "@id": "https://schema.org/Integer", 
      "@type": "@id",
      "description": "Manufacturer recommended humidty for storing the battery."
    },
    "criticalRawMaterials": {
      "@id": "https://schema.org/ItemList", 
      "@type": "@id",
      "description": "Critical raw materials contained in the battery above a concentration of 0.1 % weight by weight."
    },
    "batteryChemistry": {
      "@id": "https://schema.org/ItemList", 
      "@type": "@id",
      "description": "Composition of a battery in general terms by specifying the cathode and anode active material as well as electrolyte."
    },
    "cathodeAnodeElectrolyteNames": {
      "@id": "https://schema.org/ItemList", 
      "@type": "@id",
      "description": "Name of the materials =  {cathode material, anode material, electrolyte material} in this order."
    },
    "relatedIDsCathodeAnodeElectrolytes": {
      "@id": "https://schema.org/ItemList", 
      "@type": "@id",
      "description": "Related identifiers of the cathode, anode, electrolyte materials = {identier of cathode, identifier of anode, identifier of electrolyte}"
    },
    "weightCathodeAnodeElectrolytes": {
      "@id": "https://schema.org/ItemList", 
      "@type": "@id",
      "description": "Total weights of the cathode, anode, electrolyte materials in the pack =  {cathode weight in kg, anode weight in kg, electrolyte weight in kg}"
    },
    "hazardousSubstancesNames": {
      "@id": "https://schema.org/ItemList", 
      "@type": "@id",
      "description": "Name of hazardous substances other than mercury, cadmium or lead = {substance 1, substance 2, ... }"
    },
    "hazardSubstancesClasses": {
      "@id": "https://schema.org/ItemList", 
      "@type": "@id",
      "description": "The class hazardous substances other than mercury, cadmium or lead = {class 1, class 2, ... }"
    },
    "relatedIDHazardousSubstances": {
      "@id": "https://schema.org/ItemList", 
      "@type": "@id",
      "description": "The identifier hazardous substances other than mercury, cadmium or lead = {identier 1, identifier 2, ...}"
    },
    "locationHazardousSubstances": {
      "@id": "https://schema.org/ItemList", 
      "@type": "@id",
      "description": "The location of hazardous substances other than mercury, cadmium or lead on the battery = {substance 1: location 1, substance 2: location 2, .... }"
    },
    "concentrationRangeHazardousSubstances": {
      "@id": "https://schema.org/ItemList", 
      "@type": "@id",
      "description": "The concentration range of hazardous substances other than mercury, cadmium or lead = substance 1{lower bound, upperbound}, substance 2{lowerbound, upperbound}."
    },
    "substancesImpactDescriptions": {
      "@id": "https://schema.org/ItemList", 
      "@type": "@id",
      "description": "Description of the impact of substances on environment, health, etc."
    },
    "carbonFootprint": {
      "@id": "https://schema.org/Number", 
      "@type": "@id",
      "description": "The carbon footprint of the battery over its expected service life. Unit of Measure is kg/kWh."
    },
     "carbonFootprintShareDistribution": {
      "@id": "https://schema.org/Number", 
      "@type": "@id",
      "description": "The carbon footprint of the battery over its expected service life in distribution stage."
    },
    "carbonFootprintShareEndOfLifeRecycling": {
      "@id": "https://schema.org/Number", 
      "@type": "@id",
      "description": "The carbon footprint of the battery over its expected service life in end of life and recycling stage."
    },
     "dueDiligenceReport": {
      "@id": "https://schema.org/url", 
      "@type": "@id",
      "description": "link to due dilligence report."
    },
    "sustainabilityReport": {
      "@id": "https://schema.org/url", 
      "@type": "@id",
      "description": "link to sustainability report."
    },
    "postalAddressSparePartsSources": {
      "@id": "https://schema.org/ItemList", 
      "@type": "@id",
      "description": "Postal address of sources for replacement spares."
    },
    "emailsSparePartsSources": {
      "@id": "https://schema.org/ItemList", 
      "@type": "@id",
      "description": "E-mail address of supplier for spare parts."
    },
    "sparePartsSources": {
      "@id": "https://schema.org/ItemList", 
      "@type": "@id",
      "description": "URI of supplier for spare parts."
    },
    "extinguishingAgent": {
      "@id": "https://schema.org/Text", 
      "@type": "@id",
      "description": "link to sustainability report."
    },
     "preConsumerRecycledNickelShare": {
      "@id": "https://schema.org/Number", 
      "@type": "@id",
      "description": "Share of nickel recovered from battery manufacturing waste present in active materials for each battery model per year and per manufacturing plant."
    },
    "preConsumerRecycledCobaltShare": {
      "@id": "https://schema.org/Number", 
      "@type": "@id",
      "description": "Share of cobalt recovered from battery manufacturing waste present in active materials for each battery model per year and per manufacturing plant."
    },
    "preConsumerRecycledLithiumShare": {
      "@id": "https://schema.org/Number", 
      "@type": "@id",
      "description": "Share of lithium recovered from battery manufacturing waste present in active materials for each battery model per year and per manufacturing plant."
    },
    "postConsumerRecycledCobaltShare": {
      "@id": "https://schema.org/Number", 
      "@type": "@id",
      "description": "Share of cobalt recovered from post-consumer waste present in active materials for each battery model per year and per manufacturing plant."
    },
    "postConsumerRecycledLeadShare": {
      "@id": "https://schema.org/Number", 
      "@type": "@id",
      "description": "Share of lead recovered from waste present in the battery, for each battery model per year and per manufacturing plant."
    },
    "renewableContentShare": {
      "@id": "https://schema.org/Number", 
      "@type": "@id",
      "description": "Share of renewable content in the battery."
    },
    "roleEndUsersContributingWastePrevention": {
      "@id": "https://schema.org/url", 
      "@type": "@id",
      "description": "URI to the document about the role of end-users in contributing to waste prevention, including by information on good practices and recommendations concerning the use of batteries aiming at extending their use phase and the possibilities of re-use, preparing for re-use, preparing for repurpose, repurposing and remanufacturing."
    },
    "roleEndUsersContributingCollectionWasteBatteries": {
      "@id": "https://schema.org/url", 
      "@type": "@id",
      "description": "URI to the document about the role of end-users in contributing to the separate collection of waste batteries.."
    },
    "infoCollectionTakebackReuseRepurposingRecycling": {
      "@id": "https://schema.org/url", 
      "@type": "@id",
      "description": "URI to the document about the  separate collection, take-back and collection points, preparing for re-use, preparing for repurposing, and recycling operations available for waste batteries."
    },
    "manualBatteryDisassemblyDismantling": {
      "@id": "https://schema.org/url", 
      "@type": "@id",
      "description": "URI to the manual for dismantling of battery pack. The same attribute as ID #C7-2."
    },
    "numberOfCells": {
      "@id": "https://schema.org/Number", 
      "@type": "@id",
      "description": "Total number of cells in the pack."
    },
    "currentOwner": {
      "@id": "https://dlt.mobi/mobility-schema/schemas/Entity.json",
      "@type": "@id",
      "description": "The current owner information."
    },
    "jurisdiction": {
      "@id": "https://dlt.mobi/mobility-schema/schemas/Entity.json",
      "@type": "@id",
      "description": "The jurisdictional entity information."
    },
    "manualBatteryRemoval": {
      "@id": "https://dlt.mobi/mobility-schema/schemas/manualBatteryRemoval.json",
      "@type": "@id",
      "description": "The jurisdictional entity information."
    },
    "safetyMeasuresInstructions": {
      "@id": "https://dlt.mobi/mobility-schema/schemas/safetyMeasuresInstructions.json",
      "@type": "@id",
      "description": "The jurisdictional entity information."
    },
    "postConsumerRecycledLithiumShare": {
      "@id": "https://dlt.mobi/mobility-schema/schemas/postConsumerRecycledLithiumShare.json",
      "@type": "@id",
      "description": "The jurisdictional entity information."
    },
    "postConsumerRecycledNickelShare": {
      "@id": "https://dlt.mobi/mobility-schema/schemas/postConsumerRecycledNickelShare.json",
      "@type": "@id",
      "description": "The jurisdictional entity information."
    },
    "preConsumerRecycledLeadShare": {
      "@id": "https://dlt.mobi/mobility-schema/schemas/preConsumerRecycledLeadShare.json",
      "@type": "@id",
      "description": "The jurisdictional entity information."
    }
   
  
}